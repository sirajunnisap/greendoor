


<!DOCTYPE html>
<html lang="en">

<head>

    <!-- metas -->
    <meta charset="utf-8">
    <meta name="author" content="Chitrakoot Web" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="keywords" content="Multipurpose eCommerce Template + Admin" />
    <meta name="description" content="Smartshop - Multipurpose eCommerce Template + Admin" />

    <!-- title  -->
    <title>Smartshop - Multipurpose eCommerce Template + Admin</title>

    <!-- favicon -->
    <link rel="shortcut icon" href="/users/img/logos/favicon.png">
    <link rel="apple-touch-icon" href="/users/img/logos/apple-touch-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/users/img/logos/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/users/img/logos/apple-touch-icon-114x114.png">

    <!-- plugins -->
    <link rel="stylesheet" href="/users/css/plugins.css">

    <!-- theme core css -->
    <link href="/users/css/styles.css" rel="stylesheet">

</head>

<body>

    <!-- PAGE LOADING
    ================================================== -->
    <div id="preloader"></div>

    <!-- MAIN WRAPPER
    ================================================== -->
    <div class="main-wrapper mp-pusher" id="mp-pusher">

        <!-- HEADER
        ================================================== -->
        <header class="fixed header-light-nav">
<!-- 
           
       
        LOGIN REGISTER
        ================================================== -->
        <section class="md">
            <div class="container">
                <div class="row">

                    <div class="col-lg-6 mb-1-9 mb-lg-0">

                        <div class="common-block">


                            <form method="post">

                                <div class="row">

                                    <div class="col-sm-12">

                                        <div class="form-group">
                                            <label>Email</label>
                                            <input type="email" class="form-control" name="email" placeholder="Your email here" required="">
                                        </div>

                                    </div>

                                    <div class="col-sm-12">

                                        <div class="form-group">
                                            <label>Password </label>
                                            <input type="password" class="form-control" name="password" placeholder="Your password here" required="">
                                        </div>

                                    </div>

                                </div>


                                <button type="submit" class="butn-style2 mt-4">Login</button>

                              
                               
                                
                            </form>

                        </div>

                    </div>

                    

                </div>
            </div>
        </section>
        
       

    <!-- jQuery -->
    <script src="/users/js/jquery.min.js"></script>

    <!-- popper js -->
    <script src="/users/js/popper.min.js"></script>

    <!-- bootstrap -->
