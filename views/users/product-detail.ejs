    <script>
        function addToCart(prodId){
            $.ajax({
                url:'/addtocart',
                data : {
                    productId : prodId
                },
                method : 'post',

                success : (res) =>{
                    if(res.success){
                        Swal.fire({
                            title : 'Added',
                            text : 'product added to cart',
                            position : 'center',
                            icon : 'success',
                            showConfirmButton : false,
                            timer : 1000
                        })
                        
                    }else{
                        Swal.fire({
                            title : 'Already Exist',
                            text : 'product already in cart',
                            icon : 'info',
                            showConfirmButton :false,
                            timer:1000
                        })
                    }
                }
            })
        }
    </script>
