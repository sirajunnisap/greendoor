                            </div>
                            <div id="accordion" class="accordion-style2">
                                <% categoryData.forEach((value)=>{ %>
                                    <div class="card">
                                        <div class="card-header" id="headingOne">
                                            <a href="/shopCategory/<%=value._id %>"> <%= value.name %></a>
                                        
                                        </div>

                                    </div>
                                <% }) %>
                                
                                      
                            </div>
                        </div>
                              

                        

                    </div>
                    <!-- end sidebar panel -->

